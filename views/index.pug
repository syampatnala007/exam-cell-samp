              label#programHide program:
              input#programHide(type='text' name='program' value='btech')
              label#programHide mode:
              input#programHide(type='text' name='mode' value='labexaminor')
              label select the Regulation
              select#regulation.form-control(name='regulation', required='')
                option(value='') ---Select---
                option(value='AR17') AR17
                option(value='AR19') AR19
              br
              label select the semester
              select#sem.form-control(name='sem', required='')
                option(value='') ---Select---
                each member in seme
                  option(value=member.Semester) #{member.Semester}
              br
              label select the exam type
              select#examtype.form-control(name='examtype', required='')
                option(value='') ---Select---
                option(value='Regular') Regular
                option(value='Supply') Supply
                option(value='Regularandsupply') Regular/Supply
              br
              label select the month and date
              input#month.form-control(type='month', name='month')
